{
	"appId": "io.ionic.starter",
	"appName": "inAppBrowse",
	"webDir": "www",
	"server": {
		"androidScheme": "http",
		"cleartext": true,
		"allowNavigation": [
			"upi"
		]
	},
	"plugins": {
		"CapacitorHttp": {
			"enabled": true
		}
	},
	"cordova": {
		"accessOrigins": [
			"http",
			"upi"
		],
		"preferences": {
			"URL_SCHEME": "upi"
		}
	}
}
